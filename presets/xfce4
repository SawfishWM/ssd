;; Sawfish-Session-Dialog configuration

(defvar-setq logout-cmd "xfce4-session-logout --logout")
(defvar-setq reboot-cmd "xfce4-session-logout --reboot")
(defvar-setq shutdown-cmd "xfce4-session-logout --halt")
(defvar-setq lockdown-cmd "xflock4")
(defvar-setq suspend-cmd "xfce4-session-logout --suspend")
(defvar-setq hibernate-cmd "xfce4-session-logout --hibernate")
